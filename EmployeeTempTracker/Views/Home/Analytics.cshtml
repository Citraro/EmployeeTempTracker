<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<script type="text/javascript" src="/js/graphs.js"></script>
<div class="w3-row">
    <div class="w3-half w3-padding-right">
        <div class="w3-card-4">
            <div class="w3-container w3-green w3-padding-small w3-center"><h2>Average Temperature</h2></div>
            <div class="w3-container" id="avg_temp_graph">
                <script>
                    computeGraph(@Html.Raw(ViewData["Temperatures"]), "chart_avg_temps");
                </script>
                <div id="chart_avg_temps" ></div>
            </div>
        </div>
    </div>
    <div class="w3-half w3-padding-left">
        <div class="w3-card-4">
            <div class="w3-container w3-green w3-padding-small w3-center"><h2>Screened Employees</h2></div>
            <div class="w3-container" id="screened_employees_graph">
                <script>
                    computeGraph(@Html.Raw(ViewData["Temperatures"]), "chart_screened_employees");
                </script>
                <div id="chart_screened_employees"></div>
            </div>
        </div>
    </div>
</div>



